<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iecheck.app.scanticketdemo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:name="com.yihukurama.app.scanticketdemo.ScandemoApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.yihukurama.app.scanticketdemo.framework.v.showview.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.yihukurama.app.scanticketdemo.framework.v.showview.Detail_Activity" />
        <activity android:name="com.yihukurama.app.scanticketdemo.framework.v.showview.CuanHuoCaptureActivity" />
        <activity android:name="com.yihukurama.app.scanticketdemo.framework.v.showview.ResultActivity"></activity>
        <activity android:name="com.yihukurama.app.scanticketdemo.framework.v.showview.YunduanCaptureActivity" />
        <activity android:name="com.yihukurama.app.scanticketdemo.framework.v.showview.YunduanExchangeCaptureActivity" />
    </application>

</manifest>